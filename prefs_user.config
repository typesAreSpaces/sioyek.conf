page_separator_color #1d2021
page_separator_width 10
custom_background_color #282828
custom_text_color #fbf1c7
search_highlight_color #7ec16e
status_bar_color #427b58
status_bar_text_color #fbf1c7

inverse_search_command /opt/homebrew/bin/emacsclient -s jose --no-wait +%2 "%1"
startup_commands toggle_custom_color;toggle_synctex

new_command _path_info bash ~/.config/sioyek/scripts/status_display_info.sh "%{file_path}"
new_command _path_clipboard bash ~/.config/sioyek/scripts/cp_to_clipboard.sh "%{file_path}"
